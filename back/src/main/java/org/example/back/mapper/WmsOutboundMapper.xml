<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.back.mapper.WmsOutboundMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.example.back.domain.WmsOutbound">
        <id column="id" property="id" />
        <result column="outbound_no" property="outboundNo" />
        <result column="outbound_status" property="outboundStatus" />
        <result column="outbound_time" property="outboundTime" />
        <result column="client" property="client" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, outbound_no, outbound_status, outbound_time, client
    </sql>

</mapper>
